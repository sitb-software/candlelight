{
  "name": "candlelight",
  "version": "1.0.0-alpha.26",
  "description": "A NodeJS Web Framework",
  "main": "lib/Application.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "tsc",
    "deploy": "tsc -d",
    "watch": "tsc -w",
    "clear": "rm -rf *.d.ts *.js *.js.map **/*.d.ts **/*.js **/*.js.map",
    "prepare": "npm run clear && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitb-software/candlelight.git"
  },
  "keywords": [
    "nodejs",
    "web",
    "restful"
  ],
  "author": "Sean",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sitb-software/candlelight/issues"
  },
  "homepage": "https://github.com/sitb-software/candlelight#readme",
  "dependencies": {
    "content-type": "^1.0.4",
    "path-to-regexp": "^2.1.0",
    "raw-body": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^8.0.52",
    "typescript": "^2.6.1"
  }
}
